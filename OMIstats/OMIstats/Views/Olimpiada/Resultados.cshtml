@if (Model.mostrarResultadosPorDia || secretScoreboard && dia == 2)
{
<td class="@("table-td center right-border resultado-dia " + (descalificado ? "" : " open-overlay ") + claseCSS)" onclick="@(descalificado ? "null" : "showOverlay('" + datos.clave + "')")">
