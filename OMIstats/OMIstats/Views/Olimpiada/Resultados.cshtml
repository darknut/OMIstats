<tr id="@((isRunnerLive || Model.puntosDetallados) ? datos.clave : null)" foto="@((isRunnerLive || Model.puntosDetallados) ? datos.persona.foto.Substring(2) : null)" @(estado.extranjero ? "ext=true style=display:none" : "")>
